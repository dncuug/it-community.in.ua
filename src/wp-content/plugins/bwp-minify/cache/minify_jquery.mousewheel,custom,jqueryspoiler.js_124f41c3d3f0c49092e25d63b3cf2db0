(function($){var types=['DOMMouseScroll','mousewheel'];$.event.special.mousewheel={setup:function(){if(this.addEventListener){for(var i=types.length;i;){this.addEventListener(types[--i],handler,false);}}else{this.onmousewheel=handler;}},teardown:function(){if(this.removeEventListener){for(var i=types.length;i;){this.removeEventListener(types[--i],handler,false);}}else{this.onmousewheel=null;}}};$.fn.extend({mousewheel:function(fn){return fn?this.bind("mousewheel",fn):this.trigger("mousewheel");},unmousewheel:function(fn){return this.unbind("mousewheel",fn);}});function handler(event){var orgEvent=event||window.event,args=[].slice.call(arguments,1),delta=0,returnValue=true,deltaX=0,deltaY=0;event=$.event.fix(orgEvent);event.type="mousewheel";if(event.wheelDelta){delta=event.wheelDelta/120;}
if(event.detail){delta=-event.detail/3;}
deltaY=delta;if(orgEvent.axis!==undefined&&orgEvent.axis===orgEvent.HORIZONTAL_AXIS){deltaY=0;deltaX=-1*delta;}
if(orgEvent.wheelDeltaY!==undefined){deltaY=orgEvent.wheelDeltaY/120;}
if(orgEvent.wheelDeltaX!==undefined){deltaX=-1*orgEvent.wheelDeltaX/120;}
args.unshift(event,delta,deltaX,deltaY);return $.event.handle.apply(this,args);}})(jQuery);;jQuery(document).ready(function(){jQuery('#main-menu .menu').mobileMenu({defaultText:'Перейти к...',className:'select-menu',subMenuDash:'&nbsp;&nbsp;&nbsp;&ndash;'});jQuery('#main-menu ul.menu').superfish({hoverClass:'over',delay:100,animation:{opacity:'show',height:'show'},speed:150,autoArrows:false,dropShadows:true,delay:0});jQuery(".sidebar-carousel-posts").jCarouselLite({visible:1,auto:5000,btnNext:".wid-next",btnPrev:".wid-prev"});jQuery('.fb-posts-list').flexslider({animation:"slide",slideshow:true,directionNav:false,controlsContainer:".fb-posts-nav",controlNav:true,smoothHeight:false,animationSpeed:800});jQuery('.recent-tweets-list').flexslider({animation:"slide",slideshow:true,directionNav:false,controlsContainer:".recent-tweets-nav",controlNav:true,smoothHeight:false,animationSpeed:800});jQuery('#wt-slider').flexslider({animation:"slide",controlNav:false,slideshow:true,directionNav:true,slideshowSpeed:3000,smoothHeight:false,controlsContainer:"#wt-slider .slider-nav",});jQuery('.slide-cat1').flexslider({animation:"slide",slideshow:false,directionNav:false,controlsContainer:"#slide-cat1-nav",controlNav:true,smoothHeight:true,animationSpeed:800});jQuery('.slide-cat2').flexslider({animation:"slide",slideshow:false,directionNav:false,controlsContainer:"#slide-cat2-nav",controlNav:true,smoothHeight:true,animationSpeed:800});jQuery('.slide-cat3').flexslider({animation:"slide",slideshow:false,directionNav:false,controlsContainer:"#slide-cat3-nav",controlNav:true,smoothHeight:true,animationSpeed:800});jQuery('.slide-cat4').flexslider({animation:"slide",slideshow:false,directionNav:false,controlsContainer:"#slide-cat4-nav",controlNav:true,smoothHeight:true,animationSpeed:800});jQuery('.slide-cat5').flexslider({animation:"slide",slideshow:false,directionNav:false,controlsContainer:"#slide-cat5-nav",controlNav:true,smoothHeight:true,animationSpeed:800});jQuery(".live-tile, .fli